				"envFile": "./libs/dnd-5e-api/docker/.env"
			}
		},
		"stop": {
			"executor": "@nrwl/workspace:run-commands",
			"outputs": [],
			"options": {
				"command": "sudo docker-compose stop",
				"cwd": "./libs/dnd-5e-api/docker",
				"envFile": "./libs/dnd-5e-api/docker/.env"
