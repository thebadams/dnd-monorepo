{
	"root": "libs/dnd-5e-api/docker",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nrwl/workspace:run-commands",
			"outputs": [],
			"options": {
				"command": "sudo docker-compose up --no-start",
				"cwd": "./libs/dnd-5e-api/docker",
				"envFile": "./libs/dnd-5e-api/docker/.env"
			}
		},
		"start": {
			"executor": "@nrwl/workspace:run-commands",
			"outputs": [],
