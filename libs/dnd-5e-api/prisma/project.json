{
  "root": "libs/dnd-5e-api/prisma",
  "sourceRoot": "libs/dnd-5e-api/prisma/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/dnd-5e-api/prisma/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/dnd-5e-api/prisma"],
      "options": {
        "jestConfig": "libs/dnd-5e-api/prisma/jest.config.js",
        "passWithNoTests": true
      }
    },
		"build" : {
			"executor": "@nrwl/workspace:run-commands",
			"outputs": [],
			"options": {
				"command": "npx prisma generate",
				"cwd": "./libs/dnd-5e-api/prisma"
			}
		}
  },
  "tags": []
}
